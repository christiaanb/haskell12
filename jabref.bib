@MISC{PdistHs,
  author = {Frank Huch and Volker Stolz},
  title = {Implementation of Port-based Distributed Haskell},
  year = {2001},
  booktitle = {Draft Proceedings of IFL'01},
  optabstract = {We present an implementation of robust distributed open systems in
	Haskell via ports. The system is designed to allow an arbitrary number
	of applications to communicate in a peer-to-peer manner. An application
	may offer services to clients in the network which can access it
	over the Internet. For service-discovery, the supporting runtime-system
	(implemented itself in Haskell) can map symbolic names to the low-level
	references used for internal communication. A stream layer abstracts
